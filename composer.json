{
  "name": "talv86/easel",
  "description": "A package for the minimal blogging app 'Canvas' by Todd Austin (based off commit #3c59c55)",
  "keywords": ["blog", "laravel", "canvas", "markdown", "developers", "minimal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Talvinder Bansal",
      "email": "talvbansal@hotmail.com",
      "role": "Developer"
    },
    {
      "name": "Todd Austin",
      "email": "austin.todd.j@gmail.com",
      "role": "Developer"
    }

  ],
  "require": {
    "php": ">=5.6",
    "bestmomo/scafold": "dev-master",
    "dflydev/apache-mime-types": "^1.0",
    "doctrine/dbal": "^2.5",
    "erusev/parsedown": "^1.6",
    "proengsoft/laravel-jsvalidation": "1.0.*",
    "laravelcollective/html": "^5.2",
    "symfony/filesystem": "^3.1"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "Easel\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "minimum-stability": "dev"
}
